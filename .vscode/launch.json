{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Golang-Server",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/golang/cmd/server/main.go",
      "env": {},
      "args": ["-toml", "${workspaceFolder}/golang/toml/local.toml", "-env", "${workspaceFolder}/golang/env/.env.local"],
      "showLog": true
    },
    {
      "name": "Ticker-Batch",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/golang/cmd/ticker_batch/main.go",
      "env": {},
      "args": ["-toml", "${workspaceFolder}/golang/toml/local.toml", "-env", "${workspaceFolder}/golang/env/.env.local"],
      "showLog": true
    },
    {
      "name": "Import-Tickers",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/golang/cmd/import_tickers/main.go",
      "env": {},
      "args": [],
      "showLog": true
    },
    {
      "name": "DRF Server",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/python/drf/manage.py",
      "args": ["runserver"],
      "cwd": "${workspaceFolder}/python/drf",
      "console": "integratedTerminal",
      "justMyCode": true
    },
    {
      "name": "Fast API Server",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/python/fast_api/main.py",
      "args": [],
      "cwd": "${workspaceFolder}/python/fast_api",
      "console": "integratedTerminal",
      "justMyCode": true
    }
  ]
}
